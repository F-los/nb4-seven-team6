{
  "openapi": "3.0.4",
  "info": {
    "title": "초급팀플 운동 공유 웹 백엔드 개발",
    "version": "1.0.0",
    "description": "2025.08.11 - 2025.08.28"
  },
  "paths": {
    "/groups": {
      "post": {
        "summary": "그룹 생성",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": { "type": "string" },
                  "description": { "type": "string" },
                  "photoUrl": { "type": "string" },
                  "goalReq": { "type": "integer" },
                  "discordWebhookUrl": { "type": "string" },
                  "discordInviteUrl": { "type": "string" },
                  "tage": {
                    "type": "array",
                    "items": { "type": "string" }
                  },
                  "ownerNickname": { "type": "string" },
                  "ownerPassword": { "type": "string" }
                },
                "required": [
                  "name",
                  "description",
                  "goalReq",
                  "discordWebhookUrl",
                  "discordInviteUrl",
                  "ownerNickname",
                  "ownerPassword"
                ]
              }
            }
          }
        },
        "responses": {
          "201": { "description": "생성 성공" },
          "400": { "description": "잘못된 요청" }
        }
      }
    }
  }
}
